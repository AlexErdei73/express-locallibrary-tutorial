<%- include("_header") %>
<h1><%= title %></h1>
<ul>
	<% if (book_list.length > 0) { book_list.forEach(function(book) { %>
	<li>
		<a href="<%= book.url %>"><%= book.title %></a> (<%= book.author.name %>)
	</li>
	<% })} else { %>
	<li>there are no books.</li>
	<% } %>
</ul>
<%- include("_footer") %>
